{
	"name": "skeschema",
	"version": "1.0.0",
	"description": "",
	"main": "dist/server.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --transpile-only --exit-child --clear src/server.ts",
		"build": "tsc --build --verbose",
		"start": "node dist/server.js",
		"start:prod": "NODE_ENV=production node dist/server.js",
		"lint": "eslint . --ext .ts --max-warnings 0",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"format:check": "prettier --check \"src/**/*.ts\"",
		"type-check": "tsc --noEmit",
		"test": "jest --runInBand --detectOpenHandles --coverage",
		"test:watch": "jest --watch",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate deploy",
		"db:migrate:dev": "prisma migrate dev",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio",
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"postinstall": "prisma generate || true"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MunyinSam/SKE-Schema.git"
	},
	"author": "MunyinSam",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/MunyinSam/SKE-Schema/issues"
	},
	"homepage": "https://github.com/MunyinSam/SKE-Schema#readme",
	"dependencies": {
		"@prisma/client": "^6.19.0",
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.1",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"framer-motion": "^12.23.24",
		"helmet": "^8.1.0",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.1",
		"mssql": "^11.0.1",
		"multer": "^2.0.2",
		"nodemailer": "^7.0.10",
		"pg": "^8.16.3",
		"zod": "^4.1.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.34.0",
		"@testcontainers/mssqlserver": "^11.8.1",
		"@types/bcrypt": "^6.0.0",
		"@types/cookie-parser": "^1.4.9",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/morgan": "^1.9.10",
		"@types/mssql": "^9.1.7",
		"@types/multer": "^2.0.0",
		"@types/nodemailer": "^7.0.3",
		"@types/pg": "^8.15.6",
		"@types/socket.io": "^3.0.1",
		"@types/supertest": "^6.0.3",
		"@typescript-eslint/eslint-plugin": "^8.41.0",
		"@typescript-eslint/parser": "^8.41.0",
		"eslint": "^9.34.0",
		"jest": "^30.2.0",
		"jest-junit": "^16.0.0",
		"prettier": "^3.6.2",
		"prisma": "^6.19.0",
		"rimraf": "^6.1.2",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.5",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.41.0"
	}
}
